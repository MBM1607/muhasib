#:kivy 1.11.1

#:import MAIN_COLOR constants.MAIN_COLOR
#:import NAVIGATION_DATA constants.NAVIGATION_DATA

<Dashboard>:
	name: "dashboard"
	BoxLayout:
		orientation: "vertical"
		spacing: dp(10)
		CustomActionBar:
			Widget:
			IconButton:
				icon: "data/location.png"
				on_press: app.open_location_popup()
		WidgetGrid:
			cols: 2
			viewclass: "DashboardButton"
			data: NAVIGATION_DATA[1:]
